#include<stdio.h>
#include<conio.h>
void main()
  {
  clrscr();
  char s[50];
  printf("Enter any word to Encrypt\n"); //write a word here then enter
  scanf("%s",s);
  for(int i=0;i<=s[i];i++)
   {
     printf("%c",s[i]-2);     //The encrypted result will obtain here, Hence it is encrypted
   }
    getch();
   }
